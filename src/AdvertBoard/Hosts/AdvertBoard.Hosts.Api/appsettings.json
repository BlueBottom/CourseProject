{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Debug"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "advertboard-{0:yyyy.MM.dd}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=advertboard;Username=postgres;Password=password;Port=5432;Include Error Detail=true;",
    "RabbitMq": "amqp://guest:guest@localhost:5672/advertboard",
    "Redis": "localhost:6379"
  },
  "Jwt": {
    "Issuer": "JWTAuthenticationServer",
    "Audience": "JWTAuthenticationClient",
    "Key": "IWannaJobPleasePleasePls123123123"
  }
}